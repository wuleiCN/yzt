import request from '@/utils/request-zhgd'

export function getList(data) {
  return request({
    url: '/video/findVideoList',
    method: 'post',
    data
  })
}
export function videoMonitorList(data) {
  return request({
    url: '/video/findNewsVideoList',
    method: 'post',
    data
  })
}
export function save(data) {
  return request({
    url: '/video/saveVideo',
    method: 'post',
    data
  })
}
export function update(data) {
  return request({
    url: 'video/updVideoById',
    method: 'post',
    data
  })
}
export function findById(data) {
  return request({
    url: '/video/findById',
    method: 'post',
    data
  })
}
export function del(params) {
  return request({
    url: '/video/delVideo',
    method: 'get',
    params
  })
}
export function stopVideo(params) {
  return request({
    url: '/video/stopContro',
    method: 'get',
    params
  })
}
export function controlVideo(params) {
  return request({
    url: '/video/startContro',
    method: 'get',
    params
  })
}
export function findDeviceCapacity(params) {
  return request({
    url: '/video/findDeviceCapacity',
    method: 'get',
    params
  })
}
export function startControlHKSDK(params) {
  return request({
    url: '/video/startControlHKSDK',
    method: 'get',
    params
  })
}
export function deviceCapture(params) {
  return request({
    url: '/video/deviceCapture',
    method: 'get',
    params
  })
}
export function videoProList(params) {
  return request({
    url: '/video/screenPro',
    method: 'get',
    params
  })
}
