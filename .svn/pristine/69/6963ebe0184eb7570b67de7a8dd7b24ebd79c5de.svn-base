import request from '@/utils/request'

export function getList(data) {
  return request({
    url: '/article/list',
    method: 'post',
    data
  })
}
export function saveOrUpdate(data) {
  return request({
    url: '/article/saveOrUpdate',
    method: 'post',
    data
  })
}

export function del(data) {
  return request({
    url: '/article/delete',
    method: 'post',
    data
  })
}

