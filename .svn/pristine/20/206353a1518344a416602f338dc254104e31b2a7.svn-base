import request from '@/utils/request'

export function getList(data) {
  return request({
    url: '/product/list',
    method: 'post',
    data
  })
}
export function saveOrUpdate(data) {
  return request({
    url: '/product/saveOrUpdate',
    method: 'post',
    data
  })
}

export function del(data) {
  return request({
    url: '/product/delete',
    method: 'post',
    data
  })
}
export function productRecord(data) {
  return request({
    url: '/productRecord/list',
    method: 'post',
    data
  })
}
export function weekStatistics(data) {
  return request({
