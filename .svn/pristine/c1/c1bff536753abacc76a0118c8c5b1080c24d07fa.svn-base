import request from '@/utils/request'

export function getList(data) {
  return request({
    url: '/productPurchaseDetail/list',
    method: 'post',
    data
  })
}
export function saveOrUpdate(data) {
  return request({
    url: '/productPurchaseDetail/saveOrUpdate',
    method: 'post',
    data
  })
}
export function del(data) {
  return request({
    url: '/productPurchaseDetail/delete',
    method: 'post',
    data
  })
}

