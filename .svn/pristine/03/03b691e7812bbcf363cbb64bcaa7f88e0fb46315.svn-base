import request from '@/utils/request-zhgd'

export function getList(data) {
  return request({
    url: '/electric/list',
    method: 'post',
    data
  })
}
export function saveOrUpdateElectric(data) {
  return request({
    url: '/electric/saveOrUpdateElectric',
    method: 'post',
    data
  })
}
export function del(params) {
  return request({
    url: '/electric/delete',
    method: 'get',
    params
  })
}
