import request from '@/utils/request'

export function getProjectInfo(data) {
  return request({
    url: '/bs/projectInfo',
    method: 'post',
    data
  })
}
export function getCarousel(data) {
  return request({
    url: '/bs/getCarousel',
    method: 'post',
    data
  })
}

export function getSchedule(data) {
  return request({
    url: '/bs/getSchedule',
    method: 'post',
    data
  })
}
export function getArticle(data) {
  return request({
    url: '/bs/getArticle',
    method: 'post',
    data
  })
}

