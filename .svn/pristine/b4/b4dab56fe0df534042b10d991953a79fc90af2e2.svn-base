import request from '@/utils/request-zhgd'

export function findElectrictityBoxList(data) {
  return request({
    url: '/electricityBox/findElectrictityBoxList',
    method: 'post',
    data
  })
}
export function findElectricityBoxRecordList(data) {
  return request({
    url: '/electricityBoxRecord/findElectricityBoxRecordList',
    method: 'post',
    data
  })
}
export function findNewElectRecordById(params) {
  return request({
    url: '/electricityBoxRecord/findNewElectRecordById',
    method: 'get',
    params
  })
}
export function boxProList(params) {
  return request({
    url: '/electricityBox/screenPro',
    method: 'get',
    params
  })
}
export function boxSta(params) {
  return request({
    url: '/electricityBoxRecord/boxSta',
    method: 'get',
    params
  })
}
