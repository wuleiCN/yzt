import request from '@/utils/request'

export function getList(data) {
  return request({
    url: '/dictionaries/list',
    method: 'post',
    data
  })
}

export function saveOrUpdate(data) {
  return request({
    url: '/dictionaries/saveOrUpdate',
    method: 'post',
    data
  })
}

export function del(data) {
  return request({
    url: '/dictionaries/delete',
    method: 'post',
    data
  })
}
