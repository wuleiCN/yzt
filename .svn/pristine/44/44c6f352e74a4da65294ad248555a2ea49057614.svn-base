import request from '@/utils/request'

export function getList(data) {
  return request({
    url: '/workTypePlatform/list',
    method: 'post',
    data
  })
}

export function areaOptionList(data) {
  return request({
    url: '/workTypePlatform/areaOptionList',
    method: 'post',
    data
  })
}
export function optionList(data) {
  return request({
    url: '/thirdDic/optionList',
    method: 'post',
    data
  })
}

export function update(data) {
  return request({
    url: '/workTypePlatform/update',
    method: 'post',
    data
  })
}
