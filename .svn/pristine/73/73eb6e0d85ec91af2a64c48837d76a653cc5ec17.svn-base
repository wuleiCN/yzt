import request from '@/utils/request'

export function getList(data) {
  return request({
    url: '/trainTopic/list',
    method: 'post',
    data
  })
}

export function saveOrUpdate(data) {
  return request({
    url: '/trainTopic/saveOrUpdate',
    method: 'post',
    data
  })
}

export function del(data) {
  return request({
    url: '/trainTopic/delete',
    method: 'post',
    data
  })
}
export function optionList(data) {
  return request({
    url: '/dictionaries/optionList',
    method: 'post',
    data
  })
}
export function workersList(data) {
  return request({
    url: '/trainTopic/getWorkersList',
    method: 'post',
    data
  })
}
export function detail(data) {
  return request({
    url: '/trainTopic/detail',
    method: 'post',
    data
  })
}
export function saveImg(data) {
  return request({
    url: '/trainTopic/saveImg',
    method: 'post',
    data
  })
}
