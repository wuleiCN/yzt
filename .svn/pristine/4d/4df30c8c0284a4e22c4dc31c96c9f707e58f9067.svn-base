import request from '@/utils/request'

export function getList(data) {
  return request({
    url: '/conRewardPunishment/list',
    method: 'post',
    data
  })
}
export function save(data) {
  return request({
    url: '/conRewardPunishment/save',
    method: 'post',
    data
  })
}
export function update(data) {
  return request({
    url: '/conRewardPunishment/update',
    method: 'post',
    data
  })
}
export function del(data) {
  return request({
    url: '/conRewardPunishment/delete',
    method: 'post',
    data
  })
}

