import request from '@/utils/request'

export function getWorkerInfo(data) {
  return request({
    url: '/bs/workerInfo',
    method: 'post',
    data
  })
}
export function weekAttendance(data) {
  return request({
    url: '/bs/weekAttendance',
    method: 'post',
    data
  })
}

export function conWorkers(data) {
  return request({
    url: '/bs/conWorkers',
    method: 'post',
    data
  })
}
export function workTypeProportion(data) {
  return request({
    url: '/projectKanban/workTypeProportion',
    method: 'post',
    data
  })
}

